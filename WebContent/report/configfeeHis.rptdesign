            <xml-property name="queryText"><![CDATA[SELECT 0 AS TITLE,
       SUM(CASE
             WHEN PZF.PZFZT = '1' THEN
              PZF.PRICE * PZF.AMOUNT
             ELSE
              -PZF.PRICE * PZF.AMOUNT
           END) AS COUNT1,
       BATCH.NAME_ AS BATNAME,
       INPAT.DEPTNAME AS DEPTNAME,
       FEETYPE.TYPEDESC AS CONFIGFEETYPE
  FROM SRVS_PRESCRIPTION_FEES_HIS    PZF,
       srvs_batch         BATCH,
       SRVS_INPATIENTAREA INPAT,
       SRVS_CONFIG_FEE    FEE,
       SRVS_CONFIG_FEEtype FEETYPE
 WHERE PZF.PC_ID = BATCH.ID_
   AND PZF.DEPTCODE = INPAT.DEPTCODE
   AND FEE.COSTCODE = PZF.COSTCODE
   AND PZF.PZFZT IN ('1', '3')
   AND FEE.CONFIGFEETYPE = FEETYPE.GID
]]></xml-property>
