            <method name="beforeOpen"><![CDATA[var searchTime = this.params["searchTime"].value;
var searchBatch = this.params["searchBatch"].value;
var searchDeptName = this.params["searchDeptName"].value;

if (searchTime != null &amp;&amp; searchTime != "")
{
	this.queryText += " and to_char( PZF.PZFSQRQ,'yyyy-mm-dd') like '" + searchTime +  "%'";
}


if (searchBatch != null &amp;&amp; searchBatch != "")
{
	this.queryText += " and PZF.PC_ID  in (" + searchBatch +  ")";
}

if (searchDeptName != null &amp;&amp; searchDeptName != "")
{
     var clo = searchDeptName.split(',');
     var i =0;
     this.queryText += " and inpat.DEPTNAME in ( ";
     for(i = 0; i < searchDeptName.length ; i++)
     {
       this.queryText += "'" + clo[i] + "'";
       if(i < searchDeptName.length - 1)
       {
         this.queryText += ","
       }
     }
	this.queryText += " )";
}


this.queryText += "  GROUP BY BATCH.NAME_, INPAT.DEPTNAME, FEETYPE.TYPEDESC";
this.queryText += " order by BATCH.NAME_";]]></method>
